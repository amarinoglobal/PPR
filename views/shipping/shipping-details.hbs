<div class="container">

    <h1>{{name}}</h1>

    <hr>

    <div class="row">

        <div class="col-6">
            <p><strong>Status</strong></p><br>

            <p><strong>Equipos enviados</strong></p>
            <ul>
                {{#each equipment}}
                <li>{{name}}</li>
                {{/each}}
            </ul>
        </div>


        <div class="col-4">



            <p><strong>Latitud</strong>: {{location.coordinates.[0]}}</p><br>
            <p><strong>Longitud</strong>:{{location.coordinates.[1]}}</p>

        </div>



        <hr>



    </div>

    <a href="/envios" class="btn btn-dark">Volver</a>